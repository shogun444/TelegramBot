generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Videos{
  id Int @id @default(autoincrement())
 file_id String @unique
   file_name String 
   duration  Int 
   file_size  Int? @default(0)
   thumbnail  String?
  link String?
  message_id Int
  chat_id String     
 telegram_link  String
  mime_type  String?
  width Int?
height Int? 

}
