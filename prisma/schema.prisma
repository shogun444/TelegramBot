generator client {
  provider = "prisma-client-js"
   output   = "../node_modules/@prisma/movies-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Videos{
  id Int @id @default(autoincrement())
 file_id String @unique
   file_name String 
   duration  Int? 
   file_size  String? @default("")
   thumbnail  String?
  link String?
  message_id Int?
  chat_id String     
 telegram_link  String?
  mime_type  String?
  width Int?
height Int? 
tmdb_id       Int?
}
